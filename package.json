{
  "name": "workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve:admin": "nx serve --project admin -o",
    "serve:contractor": "nx serve --project contractor --port 4202",
    "serve:sub-contractor": "nx serve --project sub-contractor -o",
    "serve:sc": "nx serve --project sub-contractor",
    "serve:signup": "nx serve --project signup -o",
    "lint:all": "nx run-many --target=lint --all",
    "test:all": "nx run-many --target=test --all",
    "watch:all": "jest --watch",
    "build:all": "nx run-many --target=build --all",
    "lint:affected": "nx affected --target=lint",
    "test:affected": "nx affected --target=test",
    "watch:affected": "nx affected --target=test --parallel --maxParallel 100 --watch",
    "build:affected": "nx affected --target=build",
    "e2e:all": "nx run-many --target=e2e --all",
    "e2e:affected": "nx affected --target=e2e",
    "e2e:admin:watch": "nx e2e --project admin-e2e --watch",
    "e2e:contractor:watch": "nx e2e --project contractor-e2e --watch",
    "e2e:sub-contractor:watch": "nx e2e --project sub-contractor-e2e --watch",
    "e2e:signup:watch": "nx e2e --project signup-e2e --watch",
    "ci:pull-request": "npm run format:check && npm run lint:affected && npm run test:affected && npm run e2e:affected && npm run build:affected",
    "ci:release": "npm run format:check && npm run lint:all && npm run test:all && npm run e2e:all && npm run build:all",
    "analyze:admin": "nx build --project admin --stats-json && webpack-bundle-analyzer ./dist/apps/admin/stats.json",
    "analyze:contractor": "nx build --project contractor --stats-json && webpack-bundle-analyzer ./dist/apps/contractor/stats.json",
    "analyze:sub-contractor": "nx build --project sub-contractor --stats-json && webpack-bundle-analyzer ./dist/apps/sub-contractor/stats.json",
    "analyze:signup": "nx build --project signup --stats-json && webpack-bundle-analyzer ./dist/apps/signup/stats.json",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "deps": "nx dep-graph",
    "postinstall": "ngcc --properties es2015 browser module main"
  },
  "private": true,
  "dependencies": {
    "-": "^0.0.1",
    "@angular-devkit/schematics": "^13.2.6",
    "@angular-material-extensions/password-strength": "^8.2.1",
    "@angular/animations": "13.2.6",
    "@angular/cdk": "13.2.6",
    "@angular/common": "13.2.6",
    "@angular/compiler": "13.2.6",
    "@angular/core": "13.2.6",
    "@angular/fire": "^7.1.1",
    "@angular/forms": "13.2.6",
    "@angular/material": "^13.2.6",
    "@angular/material-luxon-adapter": "^13.0.2",
    "@angular/material-moment-adapter": "^13.1.1",
    "@angular/platform-browser": "13.2.6",
    "@angular/platform-browser-dynamic": "13.2.6",
    "@angular/router": "13.2.6",
    "@ngrx/component-store": "13.0.2",
    "@ngrx/effects": "13.0.2",
    "@ngrx/entity": "13.0.2",
    "@ngrx/router-store": "13.0.2",
    "@ngrx/store": "13.0.2",
    "@redux-devtools/extension": "^3.2.2",
    "@schematics/angular": "^13.2.6",
    "@types/dinero.js": "^1.9.0",
    "dinero.js": "^1.9.1",
    "firebase": "^9.1.0",
    "luxon": "^2.1.1",
    "moment": "^2.29.1",
    "rxfire": "^6.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.6",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/compiler-cli": "13.2.6",
    "@angular/language-service": "13.2.6",
    "@ngrx/schematics": "13.0.2",
    "@ngrx/store-devtools": "13.0.2",
    "@nrwl/angular": "13.9.3",
    "@nrwl/cli": "13.8.7",
    "@nrwl/cypress": "13.9.3",
    "@nrwl/eslint-plugin-nx": "13.9.3",
    "@nrwl/jest": "13.9.3",
    "@nrwl/linter": "13.9.3",
    "@nrwl/workspace": "13.9.3",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "cypress": "^8.3.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jasmine-marbles": "~0.8.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "prettier": "2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "4.5.5",
    "webpack-bundle-analyzer": "^4.5.0",
    "nx": "13.9.3"
  }
}
