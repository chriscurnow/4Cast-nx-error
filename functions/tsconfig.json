{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "commonjs",
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "skipLibCheck": true,

    "target": "es2017"
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
